/dts-v1/;

#include "skeleton.dtsi"
#include "imx6q-pinfunc.h"
#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/interrupt-controller/arm-gic.h>
#include <dt-bindings/clock/imx6qdl-clock.h>
#include <dt-bindings/input/input.h>
#include "rtx-imx6q-soc.dtsi"
#include "rtx-imx6q-q7-iomux.dtsi"

/ {
	model = "Retronix i.MX6 Quad Q7 Board";
	compatible = "rtx,rtx-imx6q-q7", "fsl,imx6q";

	aliases {
		mxcfb0 = &mxcfb1;
		mxcfb1 = &mxcfb2;
		mxcfb2 = &mxcfb3;
		mxcfb3 = &mxcfb4;
	};

	memory {
		reg = <0x10000000 0x40000000>;
	};

	clocks {
		clk24m: clk24m {
			compatible = "fixed-clock";
			#clock-cells = <0>;
			clock-frequency = <24000000>;
		};
	};

	regulators {
		compatible = "simple-bus";
		#address-cells = <1>;
		#size-cells = <0>;

		reg_1p8v: 1p8v {
			compatible = "regulator-fixed";
			regulator-name = "1P8V";
			regulator-min-microvolt = <1800000>;
			regulator-max-microvolt = <1800000>;
			regulator-always-on;
		};

		reg_2p5v: 2p5v {
			compatible = "regulator-fixed";
			regulator-name = "2P5V";
			regulator-min-microvolt = <2500000>;
			regulator-max-microvolt = <2500000>;
			regulator-always-on;
		};

		reg_3p3v: 3p3v {
			compatible = "regulator-fixed";
			regulator-name = "3P3V";
			regulator-min-microvolt = <3300000>;
			regulator-max-microvolt = <3300000>;
			regulator-always-on;
		};

		reg_usb_otg_vbus: usb_otg_vbus {
			compatible = "regulator-fixed";
			regulator-name = "usb_otg_vbus";
			regulator-min-microvolt = <5000000>;
			regulator-max-microvolt = <5000000>;
			gpio = <&gpio3 22 0>;
			enable-active-high;
		};

		reg_usb_h1_vbus: usb_h1_vbus {
			compatible = "regulator-fixed";
			regulator-name = "usb_h1_vbus";
			regulator-min-microvolt = <5000000>;
			regulator-max-microvolt = <5000000>;
			gpio = <&gpio1 29 0>;
			enable-active-high;
		};

		reg_backlight: backlight {
			compatible = "regulator-fixed";
			regulator-name = "lcd-supply";
			gpio = <&gpio6 15 0>;
			startup-delay-us = <5000>;
			enable-active-high;
		};

	};

	gpio-keys {
		compatible = "gpio-keys";
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_keys_q7>;

		power {
			label = "Power Button";
			gpios = <&gpio3 29 1>;
			linux,code = <KEY_POWER>; /* KEY_POWER */
			gpio-key,wakeup;
		};
		
		volume-up {
			label = "Volume Up";
			gpios = <&gpio1 4 1>;
			gpio-key,wakeup;
			linux,code = <KEY_VOLUMEUP>;
		};

		volume-down {
			label = "Volume Down";
			gpios = <&gpio1 5 1>;
			gpio-key,wakeup;
			linux,code = <KEY_VOLUMEDOWN>;
		};
		
		home {
			label = "Home";
			gpios = <&gpio6 16 1>;
			gpio-key,wakeup;
			linux,code = <KEY_HOME>;
		};

		back {
			label = "Back";
			gpios = <&gpio1 2 1>;
			gpio-key,wakeup;
			linux,code = <KEY_BACK>;
		};
	};

	gpio_export {
		compatible = "gpio-export";

		/* current_gpio1 = <&gpio5 2 0>; 130 */
		current_gpio1 {
			/*gpio-export,name = "current_gpio0";*/
			gpio-export,output = <1>;
			gpio-export,direction_may_change ;
			gpios = <&gpio5 2 1>;
		};

		/* current_gpio2 = <&gpio3 18 0>; 82 */
		current_gpio2 {
			/*gpio-export,name = "current_gpio2";*/
			gpio-export,output = <1>;
			gpio-export,direction_may_change ;
			gpios = <&gpio3 18 1>;
		};

		/* current_gpio3 = <&gpio3 19 0>; 83 */
		current_gpio3 {
			/*gpio-export,name = "current_gpio3";*/
			gpio-export,output = <1>;
			gpio-export,direction_may_change ;
			gpios = <&gpio3 19 1>;
		};
					
		/* current_gpio4 = <&gpio4 8 0>; 104 */
		current_gpio4 {
			/*gpio-export,name = "current_gpio4";*/
			gpio-export,output = <1>;
			gpio-export,direction_may_change ;
			gpios = <&gpio4 8 1>;
		};	

		/* current_gpio5 = <&gpio3 24 0>; 88 */
		current_gpio5 {
			/*gpio-export,name = "current_gpio5";*/
			gpio-export,output = <1>;
			gpio-export,direction_may_change ;
			gpios = <&gpio3 24 1>;
		};	

		/* current_gpio6 = <&gpio3 25 0>; 89 */
		current_gpio6 {
			/*gpio-export,name = "current_gpio6";*/
			gpio-export,output = <1>;
			gpio-export,direction_may_change ;
			gpios = <&gpio3 25 1>;
		};	

		/* current_gpio7 = <&gpio3 27 0>; 91 */
		current_gpio7 {
			/*gpio-export,name = "current_gpio7";*/
			gpio-export,output = <1>;
			gpio-export,direction_may_change ;
			gpios = <&gpio3 27 1>;
		};
		
		/* current_gpio7 = <&gpio4 9 0>; 105 */
		current_gpio8 {
			/*gpio-export,name = "current_gpio8";*/
			gpio-export,output = <1>;
			gpio-export,direction_may_change ;
			gpios = <&gpio4 9 1>;
		};

		/* fan_thrm = <&gpio3 16 0>; 80 */
		fan_thrm {
			/*gpio-export,name = "fan_thrm";*/
			gpio-export,output = <1>;
			gpio-export,direction_may_change ;
			gpios = <&gpio3 16 1>;
		};

		/* fan_thrm_trip = <&gpio3 17 0>; 81 */
		fan_thrm_trip {
			/*gpio-export,name = "fan_thrm_trip";*/
			gpio-export,output = <1>;
			gpio-export,direction_may_change ;
			gpios = <&gpio3 17 1>;
		};

		/* fan_tachoin = <&gpio1 16 0>; 16 */
		fan_tachoin {
			/*gpio-export,name = "fan_tachoin";*/
			gpio-export,output = <0>;
			gpio-export,direction_may_change ;
			gpios = <&gpio1 16 1>;
		};

		/* touch_int = <&gpio6 7 0>; 16 */
		touch_int {
			/*gpio-export,name = "touch_int";*/
			gpio-export,output = <1>;
			gpio-export,direction_may_change ;
			gpios = <&gpio6 7 1>;
		};
		
		/* touch_reset = <&gpio6 8 0>; 81 */
		touch_reset {
			/*gpio-export,name = "touch_reset";*/
			gpio-export,output = <1>;
			gpio-export,direction_may_change ;
			gpios = <&gpio6 8 1>;
		};
		
		/* amp_standby = <&gpio4 10 0>; 106 */
		amp_standby {
			/*gpio-export,name = "amp_standby";*/
			gpio-export,output = <1>;
			gpio-export,direction_may_change ;
			gpios = <&gpio4 10 1>;
		};

	};

	sound {
		compatible = "fsl,imx6q-rtx-q7-sgtl5000",
			     "fsl,imx-audio-sgtl5000";
		model = "imx6q-rtx-q7-sgtl5000";
		cpu-dai = <&ssi1>;
		audio-codec = <&codec>;
		audio-routing =
			"MIC_IN", "Mic Jack",
			"LINE_IN", "Line In Jack",
			"Headphone Jack", "HP_OUT",
			"Ext Spk", "LINE_OUT";
		mux-int-port = <1>;
		mux-ext-port = <3>;
		hp-det-gpios = <&gpio7 8 1>;
		mic-det-gpios = <&gpio1 9 1>;
	};

	sound-hdmi {
		compatible = "fsl,imx6q-audio-hdmi",
			     "fsl,imx-audio-hdmi";
		model = "imx-audio-hdmi";
		hdmi-controller = <&hdmi_audio>;
	};

	lvds0_pwm {
		compatible = "pwm-backlight";
		pwms = <&pwm1 0 5000000>;
		brightness-levels = <0 4 8 16 32 64 128 255>;
		default-brightness-level = <6>;
		enable-gpios = <&gpio6 14 0>;
		power-supply = <&reg_backlight>;
		status = "okay";
	};

	fan_pwm {
		compatible = "pwm-backlight";
		pwms = <&pwm3 0 5000000>;
		brightness-levels = <0 4 8 16 32 64 128 192 224 240 248 252 255>;
		default-brightness-level = <6>;
		power-supply = <&reg_backlight>;
		status = "okay";
	};

	hannstar_cabc {
		compatible = "hannstar,cabc";

		lvds0 {
			gpios = <&gpio6 14 GPIO_ACTIVE_HIGH>;
		};
		lvds1 {
			gpios = <&gpio6 15 GPIO_ACTIVE_HIGH>;
		};
	};

	mxcfb1: fb@0 {
		compatible = "fsl,mxc_sdc_fb";
		disp_dev = "ldb";
		interface_pix_fmt = "RGB666";
		default_bpp = <16>;
		int_clk = <0>;
		late_init = <0>;
		status = "okay";
	};

	mxcfb2: fb@1 {
		compatible = "fsl,mxc_sdc_fb";
		disp_dev = "ldb";
		interface_pix_fmt = "RGB666";
		default_bpp = <16>;
		int_clk = <0>;
		late_init = <0>;
		status = "okay";
	};

	mxcfb3: fb@2 {
		compatible = "fsl,mxc_sdc_fb";
		disp_dev = "hdmi";
		interface_pix_fmt = "RGB24";
		mode_str ="1920x1080M@60";
		default_bpp = <24>;
		int_clk = <0>;
		late_init = <0>;
		status = "okay";
	};

	mxcfb4: fb@3 {
		compatible = "fsl,mxc_sdc_fb";
		disp_dev = "lcd";
		interface_pix_fmt = "RGB565";
		mode_str ="CLAA-WVGA";
		default_bpp = <16>;
		int_clk = <0>;
		late_init = <0>;
		status = "okay";
	};
/*
	lcd@0 {
		compatible = "fsl,lcd";
		ipu_id = <1>;
		disp_id = <1>;
		default_ifmt = "RGB565";
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_ipu1_q7>;
		status = "okay";
	};

	v4l2_cap_0 {
		compatible = "fsl,imx6q-v4l2-capture";
		ipu_id = <0>;
		csi_id = <0>;
		mclk_source = <0>;
		status = "okay";
	};

	v4l2_cap_1 {
		compatible = "fsl,imx6q-v4l2-capture";
		ipu_id = <0>;
		csi_id = <1>;
		mclk_source = <0>;
		status = "okay";
	};

	v4l2_out {
		compatible = "fsl,mxc_v4l2_output";
		status = "okay";
	};
*/
};

&audmux {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_audmux_q7>;
	status = "okay";
};

&cpu0 {
	arm-supply = <&reg_arm>;
	soc-supply = <&reg_soc>;
};

&ecspi4 {
	fsl,spi-num-chipselects = <1>;
	cs-gpios = <&gpio3 20 0>;
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_cspi4_q7>;
	status = "okay"; /* pin conflict with USDHC3 */

	flash: en25q32b@0 {
		compatible = "fsl,en25q32b";
		spi-max-frequency = <50000000>;
		reg = <0>;
	};
};

&fec {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_enet_q7>;
	phy-mode = "rgmii";
	phy-reset-gpios = <&gpio1 25 0>;
	phy-reset-fall-duration = <1>;
	phy-reset-rise-duration = <20>;
	phy-reset-boot-on;
	status = "okay";
};

&can1 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_flexcan1_q7>;
	trx-stby-gpio = <&gpio4 5 GPIO_ACTIVE_LOW>;
	
	status = "okay";
};

&gpc {
	fsl,ldo-bypass = <0>; /* use ldo-bypass, u-boot will check it and configure */
	fsl,wdog-reset = <2>; /* watchdog select of reset source */
};

&hdmi_audio {
	status = "okay";
};

&hdmi_cec {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_hdmi_cec_q7>;
	status = "okay";
};

&hdmi_core {
	ipu_id = <1>;
	disp_id = <0>;
	status = "okay";
};

&hdmi_video {
	fsl,phy_reg_vlev = <0x0294>;
	fsl,phy_reg_cksymtx = <0x800d>;
	status = "okay";
};

&i2c1 {
	clock-frequency = <100000>;
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_i2c1_q7>;
	status = "okay";

	codec: sgtl5000@0a {
		compatible = "fsl,sgtl5000";
		reg = <0x0a>;
		clocks = <&clks IMX6QDL_CLK_CKO>;
		VDDA-supply = <&reg_2p5v>;
		VDDIO-supply = <&reg_3p3v>;
		amic-mono;
	};
	
	mma8451@1c {
		compatible = "fsl,mma8451";
		reg = <0x1c>;
		position = <0>;
		interrupt-parent = <&gpio1>;
		interrupts = <18 8>;
		interrupt-route = <1>;
	};
};

&i2c2 {
	clock-frequency = <100000>;
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_i2c2_q7>;
	status = "okay";

	max11801@48 {
		compatible = "maxim,max11801";
		reg = <0x48>;
		interrupt-parent = <&gpio3>;
		interrupts = <26 2>;
		work-mode = <1>;/*DCM mode*/
	};

	hdmi_edid: edid@50 {
		compatible = "fsl,imx6-hdmi-i2c";
		reg = <0x50>;
	};
};

&i2c3 {
	clock-frequency = <100000>;
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_i2c3_q7>;
	status = "okay";


	efm32: rtx-efm32@0c {
		compatible = "rtx,efm32-q7";
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_mcu_q7>;
		reg = <0x0c>;
		gpio-power = <&gpio4 7 0>;
		gpio-boot = <&gpio4 10 0>;
		gpio-reset = <&gpio4 6 0>;
	};
};

&ldb {
	status = "okay";
	
	lvds-channel@0 {
		fsl,data-mapping = "spwg";
		fsl,data-width = <18>;
		primary;
		crtc = "ipu1-di0";
		interface_pix_fmt = "RGB666";
		status = "okay";

		display-timings {
			native-mode = <&timing0>;
			timing0: hsd100pxn1 {
				clock-frequency = <51208000>;
				hactive = <1024>;
				hfront-porch = <80>;
				hback-porch = <160>;
				hsync-len = <80>;
				vactive = <600>;
				vfront-porch = <10>;
				vback-porch = <18>;
				vsync-len = <7>;
				mode-is-detailed ;
				de-active = <1>;
			};
		};
	};

	lvds-channel@1 {
		fsl,data-mapping = "spwg";
		fsl,data-width = <18>;
		primary;
		crtc = "ipu1-di1";
		interface_pix_fmt = "RGB666";
		status = "okay";

		display-timings {
			native-mode = <&timing1>;
			timing1: hsd100pxn1 {
				clock-frequency = <51208000>;
				hactive = <1024>;
				hfront-porch = <80>;
				hback-porch = <160>;
				hsync-len = <80>;
				vactive = <600>;
				vfront-porch = <10>;
				vback-porch = <18>;
				vsync-len = <7>;
				mode-is-detailed ;
				de-active = <1>;
			};
		};
	};
};

&pcie {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_pcie_q7>;
	reset-gpio = <&gpio7 12 0>;
	status = "okay";
};

&pwm1 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_pwm1_q7>;
	status = "okay";
};

&pwm3 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_pwm3_q7>;
	status = "okay";
};

&ssi1 {
	fsl,mode = "i2s-slave";
	status = "okay";
};

&uart1 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_uart1_q7>;
	status = "okay";
};

&uart4 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_uart4_q7>;
	fsl,uart-has-rtscts;
	status = "okay";
};

&uart5 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_uart5_q7>;
	fsl,uart-has-rtscts;
	status = "okay";
};

&usbotg {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_usbotg_q7>;
	vbus-supply = <&reg_usb_otg_vbus>;
	disable-over-current;
	status = "okay";
};

&usbh1 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_usbh1_q7>;
	vbus-supply = <&reg_usb_h1_vbus>;
	status = "okay";
};

&usdhc2 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_usdhc2_q7_50mhz &pinctrl_usdhc2_cd_wp_q7>;
	bus-width = <4>;
	cd-gpios = <&gpio2 2 1>;
	wp-gpios = <&gpio2 3 0>;
	no-1-8-v;
	keep-power-in-suspend;
	enable-sdio-wakeup;
	status = "okay";
};

&usdhc3 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_usdhc3_q7_50mhz &pinctrl_usdhc3_cd_wp_q7>;
	cd-gpios = <&gpio2 0 1>;
	wp-gpios = <&gpio2 1 0>;
	no-1-8-v;
	keep-power-in-suspend;
	enable-sdio-wakeup;
	status = "okay";
};

&usdhc4 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_usdhc4_q7>;
	bus-width = <8>;
	non-removable;
	no-1-8-v;
	keep-power-in-suspend;
	status = "okay";
};

